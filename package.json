{
  "name": "dm-chall",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "",
  "author": "Geovani da Silva Junior",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "test": "yarn run build && mocha --exit --timeout 10000 dist/tests/**/*.js",
    "coverage": "yarn run build && nyc yarn test",
    "start": "yarn run build && yarn run watch",
    "debug": "yarn run build && yarn run watch-debug",
    "build": "yarn run build-ts",
    "build-ts": "yarn tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve\"",
    "watch-ts": "tsc -w",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\"",
    "serve": "nodemon --delay 5 dist/src/index.js",
    "serve-debug": "nodemon --inspect dist/src/index.js",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": ""
  },
  "dependencies": {
    "amqplib": "^0.8.0",
    "cors": "^2.8.5",
    "csv": "^5.5.3",
    "db-migrate": "^1.0.0-beta.16",
    "db-migrate-pg": "^1.2.2",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "fp-ts": "^2.11.5",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/chai": "^4.2.22",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.4",
    "@types/pg": "^8.6.1",
    "@types/pg-format": "^1.0.2",
    "@types/supertest": "^2.0.11",
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "concurrently": "^6.3.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
